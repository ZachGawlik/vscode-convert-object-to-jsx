{
	"name": "convert-object-to-jsx",
	"displayName": "convert-object-to-jsx",
	"description": "Convert between object entries and jsx props",
	"publisher": "zachgawlik",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.convertObjectToJsx"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.convertObjectToJsx",
				"title": "Convert Object to JSX"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test",
		"format": "yarn run prettier 'src/**/*.ts' README.md --write"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"prettier": "^1.16.0",
		"tslint": "^5.8.0",
		"tslint-config-prettier": "^1.17.0",
		"tslint-plugin-prettier": "^2.0.1",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	}
}
