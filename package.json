{
	"name": "convert-object-to-jsx",
	"displayName": "convert-object-to-jsx",
	"description": "Convert between object entries and jsx props",
	"publisher": "zachgawlik",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.convertObjectToJsx"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.convertObjectToJsx",
			"title": "Convert Object to JSX"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
